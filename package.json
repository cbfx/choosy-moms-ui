{
  "name": "choosy-moms-ui",
  "version": "0.0.6",
  "description": "choosy-moms-ui",
  "main": "index.js",
  "author": "cbfx",
  "repository": "https://github.com/cbfx/choosy-moms-ui",
  "license": "MIT",
  "engines": {
    "node": "10.8.0",
    "npm": "6.2.0"
  },
  "config": {
    "devserver": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "mockconfig": "./webpack.mock.config.js",
    "karma": "./node_modules/karma/bin/karma",
    "protractor": "./node_modules/protractor/bin/protractor",
    "jscs": "./node_modules/jscs/bin/jscs"
  },
  "scripts": {
    "changelog": "node ./node_modules/grunt/bin/grunt changelog ./Gruntfile.js",
    "start": "env USE_MOCKS=true $npm_package_config_devserver --hot --config $npm_package_config_mockconfig",
    "dist": "env USE_MOCKS=false node node_modules/grunt-cli/bin/grunt default",
    "test": "env USE_MOCKS=true $npm_package_config_karma start ./karma.conf.js --singleRun false",
    "testOnce": "env USE_MOCKS=true $npm_package_config_karma start ./karma.conf.js",
    "e2e": "node $npm_package_config_protractor protractor.sample-project.conf.js",
    "lint": "node $npm_package_config_jscs ./src --config=./racker.jscsrc",
    "postversion": "./scripts/trigger-production-deploy.sh"
  },
  "dependencies": {
    "angular": "1.6.3",
    "angular-cookies": "1.6.3",
    "angular-formly": "8.4.1",
    "angular-formly-templates-bootstrap": "6.4.0",
