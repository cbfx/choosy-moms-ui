    "grunt-contrib-copy": "1.0.0",
    "grunt-webpack": "3.0.2",
    "html-loader": "0.5.1",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "jscs": "3.0.7",
    "json-loader": "0.5.7",
